issn = {1096-0856},
journal = {Journal of magnetic resonance (San Diego, Calif. : 1997)},
keywords = {Algorithms,Computer Simulation,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Models, Chemical,Porosity,Sensitivity and Specificity},
month = {oct},
number = {2},
pages = {313--20},
