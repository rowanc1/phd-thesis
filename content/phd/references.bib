@article{Richards1931,
abstract = {The flow of liquids in unsaturated porous mediums follows the ordinary laws of hydrodynamics, the motion being produced by gravity and the pressure gradient force acting in the liquid. By making use of Darcey's law, that flow is proportional to the forces producing flow, the equation K2 +K +gK/z = sA /t may be derived for the capillary conduction of liquids in porous mediums. It is possible experimentally to determine the capillary potential =dp/ , the capillary conductivity K, which is defined by the flow equation q=K(g ), and the capillary capacity A, which is the rate of change of the liquid content of the medium with respect to . These variables are analogous, respectively, to the temperature, thermal conductivity, and thermal capacity in the case of heat flow. Data are presented and application of the equations is made for the capillary conduction of water through soil and clay but the mathematical formulations and the experimental methods developed may be used to express capillary flow for other liquids and mediums. The possible existance of a hysteresis effect between the capillary potential and moisture content of a porous medium is considered.},
archivePrefix = {arXiv},
arxivId = {http://dx.doi.org/10.1063/1.1745010},
author = {Richards, L. A.},
doi = {10.1063/1.1745010},
eprint = {/dx.doi.org/10.1063/1.1745010},
file = {:Users/rowan/Documents/Mendeley Desktop/Richards/Richards - 1931 - Capillary conduction of liquids through porous mediums.pdf:pdf},
isbn = {0148-6349},
issn = {01486349},
journal = {Journal of Applied Physics},
number = {5},
pages = {318--333},
primaryClass = {http:},
title = {{Capillary conduction of liquids through porous mediums}},
volume = {1},
year = {1931}
}
@article{Lehmann2013,
abstract = {The hydrological state of a hillslope prior to a sprinkling-induced$\backslash$nshallow landslide was monitored using electrical resistivity tomography$\backslash$n(ERT) along a 47 m long transect, supplemented by local time-domain$\backslash$nreflectometry (TDR) and tensiometer measurements. The spatial and$\backslash$ntemporal evolution of wetting patterns in the soil material indicated$\backslash$nattainment of a stationary fully saturated profile in a slope region$\backslash$nunderlain by shallow sandstone bedrock. The significant decrease$\backslash$nin spatially averaged standard deviation of water saturation has$\backslash$nnot been observed during an earlier failed attempt to trigger a landslide$\backslash$nby intense sprinkling. While for the stable experiment (no landslide$\backslash$nwas triggered) water saturation and soil moisture variability were$\backslash$nstill increasing with time, the unstable experiment reached a time-invariant$\backslash$nstate of high pore water pressures and saturations, until it finally$\backslash$nfailed. The results indicate that when large and interconnected regions$\backslash$nof hillslope are saturated (as confirmed by high volumetric water$\backslash$ncontent and low standard deviation of water saturation), additional$\backslash$nwater cannot be redistributed to empty drier regions and may eventually$\backslash$nenhance local pore water pressure and seepage force, initiating large$\backslash$nshear deformation and failure. Accordingly, a transition to such$\backslash$na critical steady state of high average water saturation, associated$\backslash$nwith low and constant spatial standard deviation, may serve as additional$\backslash$nhydro-geophysical indicator for the imminence of a landslide release.},
author = {Lehmann, Peter and Gambazzi, Francesca and Suski, Barbara and Baron, Ludovic and Askarinejad, Amin and Springman, Sarah M. and Holliger, Klaus and Or, Dani},
doi = {10.1002/2013WR014560},
file = {:Users/rowan/Documents/Mendeley Desktop/Lehmann et al/Lehmann et al. - 2013 - Evolution of soil wetting patterns preceding a hydrologically induced landslide inferred from electrical resisti.pdf:pdf},
issn = {00431397},
journal = {Water Resources Research},
number = {12},
pages = {7992--8004},
title = {{Evolution of soil wetting patterns preceding a hydrologically induced landslide inferred from electrical resistivity survey and point measurements of volumetric water content and pore water pressure}},
volume = {49},
year = {2013}
}
