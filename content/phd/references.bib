isbn = {1539-1663},
issn = {1539-1663},
journal = {Vadose Zone Journal},
number = {1},
pages = {287},
title = {{Joint Hydrological–Geophysical Inversion for Soil Structure Identification}},
url = {https://www.soils.org/publications/vzj/abstracts/7/1/287},
volume = {7},
year = {2008}
}
