@article{Liu2017,
abstract = {Bayesian inverse modeling techniques are computationally expensive because many forward simulations are needed when sampling the posterior distribution of the parameters. In this paper, we combine the implicit sampling method and generalized polynomial chaos expansion (gPCE) to significantly reduce the computational cost of performing Bayesian inverse modeling. There are three steps in this approach: (1) find the maximizer of the likelihood function using deterministic approaches; (2) construct a gPCE-based surrogate model using the results from a limited number of forward simulations; and (3) efficiently sample the posterior distribution of the parameters using implicit sampling method. The cost of constructing the gPCE-based surrogate model is further decreased by using sparse Bayesian learning to reduce the number of gPCE coefficients that have to be determined. We demonstrate the approach for a synthetic ponded infiltration experiment simulated with TOUGH2. The surrogate model is highly accurate with mean relative error that is {\textless}0.035{\%} in predicting saturation and {\textless}0.25{\%} in predicting the likelihood function. The posterior distribution of the parameters obtained using our proposed technique is nearly indistinguishable from the results obtained from either an implicit sampling method or a Markov chain Monte Carlo method utilizing the full model.},
author = {Liu, Yaning and Pau, George Shu Heng and Finsterle, Stefan},
doi = {10.1016/j.cageo.2017.04.001},
file = {:Users/rowan/Documents/Mendeley Desktop/Liu, Pau, Finsterle/Liu, Pau, Finsterle - 2017 - Implicit sampling combined with reduced order modeling for the inversion of vadose zone hydrological data.pdf:pdf},
issn = {00983004},
journal = {Computers {\&} Geosciences},
keywords = {Implicit sampling,Inverse modeling,Polynomial chaos expansion,Sparse Bayesian learning,TOUGH2,Vadose zone},
number = {May 2016},
pages = {21--32},
publisher = {Elsevier Ltd},
title = {{Implicit sampling combined with reduced order modeling for the inversion of vadose zone hydrological data}},
url = {http://dx.doi.org/10.1016/j.cageo.2017.04.001},
volume = {108},
year = {2017}
}
